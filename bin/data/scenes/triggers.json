        "ScriptingComponent#InputEventHandler": {
            "scriptFile": "",
            "scriptSource": "function InputEventHandler(event)\n\t--print(event.keyCode)\n\t--key = q\n\tif event.keyCode == 20 then\n\t\tc = Manager:getComponent(\"MovingBody\", \"Slider\");\n\t\tif c then\n\t\t\tc:setMotorVelocity(1)\n\t\tend\n\tend\n\n\t--c = Manager:getComponent(\"OmniLight\", \"LightComponent\");\n    --if c then\n    --    c.isEnabled = not c.isEnabled;\n    --end\n\n    --c = Manager:getComponent(\"Swing1\", \"Hinge\");\n    --if c then\n    --    c:setMotorVelocity(5);\n    --end\n\n    --c = Manager:getComponent(\"Door1\", \"Hinge\");\n    --if c then\n    --    c:toggleState();\n    --end\nend\n\nManager:registerEventHandler(\"InputEvent\", InputEventHandler);"
        },
        "ScriptingComponent#PhysicsPickingEventHandler": {
            "scriptFile": "",
            "scriptSource": "function PhysicsPickingEventHandler(event)\n    if event.entityName == \"Door1\" then\n        c = Manager:getComponent(\"Door1\", \"Hinge\");\n        if (c and event.distance < 2) then\n            c:toggleState();\n        end\n    end\nend\n\nManager:registerEventHandler(\"PhysicsPickingEvent\", PhysicsPickingEventHandler);"
        },
